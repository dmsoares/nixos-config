// Zed keymap
//
// For information on binding keys, see the Zed
// documentation: https://zed.dev/docs/key-bindings
//
// To see the default key bindings run `zed: open default keymap`
// from the command palette.
[
  {
    "context": "Workspace",
    "bindings": {
      "shift-space w v": "pane::SplitRight",
      "shift-space w s": "pane::SplitDown",
      "shift-space E": "diagnostics::Deploy",
      "shift-space e k": "editor::GoToPreviousDiagnostic",
      "shift-space e j": "editor::GoToDiagnostic",
      "shift-space e e": ["workspace::SendKeystrokes", "g h"],
      "shift-space e f": "editor::ToggleCodeActions",
      "shift-space e w": "diagnostics::ToggleWarnings",
      "shift-space w h": "workspace::ActivatePaneLeft",
      "shift-space w j": "workspace::ActivatePaneDown",
      "shift-space w k": "workspace::ActivatePaneUp",
      "shift-space w l": "workspace::ActivatePaneRight",
      "shift-space w H": "workspace::SwapPaneLeft",
      "shift-space w J": "workspace::SwapPaneDown",
      "shift-space w L": "workspace::SwapPaneRight",
      "shift-space w K": "workspace::SwapPaneUp",
      "shift-space w d": "pane::CloseAllItems",
      "shift-space b D": "pane::CloseInactiveItems",
      "shift-space b d": "pane::CloseActiveItem",
      "shift-space f f": "project_panel::ToggleFocus",
      "shift-space t t": "workspace::ToggleLeftDock",
      "shift-space T c": "workspace::ToggleCenteredLayout",
      "shift-space p p": "projects::OpenRecent",
      "shift-space b o": "pane::ActivatePreviousItem",
      "shift-space b i": "pane::ActivateNextItem",
      "shift-space b l": ["workspace::SendKeystrokes", "shift-space tab enter"],
      "shift-space /": "workspace::NewSearch",
      "shift-space S": "project_symbols::Toggle",
      "shift-space s": "outline::Toggle",
      "shift-space tab": "tab_switcher::Toggle"
    }
  },
  {
    "context": "Editor && vim_mode == normal && (vim_operator == none || vim_operator == n) && !VimWaiting",
    "bindings": {
      "alt-j": "vim::LineDown",
      "alt-k": "vim::LineUp",
      "space w v": "pane::SplitRight",
      "space w s": "pane::SplitDown",
      "space E": "diagnostics::Deploy",
      "space e k": "editor::GoToPreviousDiagnostic",
      "space e j": "editor::GoToDiagnostic",
      "space e e": ["workspace::SendKeystrokes", "g h"],
      "space e f": "editor::ToggleCodeActions",
      "space e w": "diagnostics::ToggleWarnings",
      "space w h": "workspace::ActivatePaneLeft",
      "space w j": "workspace::ActivatePaneDown",
      "space w k": "workspace::ActivatePaneUp",
      "space w l": "workspace::ActivatePaneRight",
      "space w H": "workspace::SwapPaneLeft",
      "space w J": "workspace::SwapPaneDown",
      "space w L": "workspace::SwapPaneRight",
      "space w K": "workspace::SwapPaneUp",
      "space w d": "pane::CloseAllItems",
      "space b D": "pane::CloseInactiveItems",
      "space b d": "pane::CloseActiveItem",
      "space b l": ["workspace::SendKeystrokes", "space tab enter"],
      "space b b": "tab_switcher::Toggle",
      "space tab": "tab_switcher::Toggle",
      "space f f": "project_panel::ToggleFocus",
      "space g g": "git_panel::ToggleFocus",
      "space t t": "workspace::ToggleLeftDock",
      "space T c": "workspace::ToggleCenteredLayout",
      "space p p": "projects::OpenRecent",
      "space b o": "pane::ActivatePreviousItem",
      "space b i": "pane::ActivateNextItem",
      "space /": "workspace::NewSearch",
      "space S": "project_symbols::Toggle",
      "space s": "outline::Toggle",
      "space g b": "editor::ToggleGitBlameInline",
      "space g B": "git::Blame"
    }
  },
  {
    "context": "Editor && (showing_code_actions || showing_completions)",
    "bindings": {
      "up": "editor::ContextMenuPrevious",
      "ctrl-k": "editor::ContextMenuPrevious",
      "down": "editor::ContextMenuNext",
      "ctrl-j": "editor::ContextMenuNext",
      "ctrl-h": "editor::ContextMenuFirst",
      "ctrl-l": "editor::ContextMenuLast"
    }
  },
  {
    "context": "Picker || menu",
    "bindings": {
      "ctrl-k": "menu::SelectPrevious",
      "ctrl-j": "menu::SelectNext",
      "ctrl-h": "menu::SelectFirst",
      "ctrl-l": "menu::SelectLast"
    }
  },
  {
    "context": "Prompt",
    "bindings": {
      "ctrl-h": "menu::SelectPrevious",
      "ctrl-l": "menu::SelectNext"
    }
  },
  {
    "context": "ProjectPanel && !editing",
    "bindings": {
      "a": "project_panel::NewFile",
      "d": "project_panel::NewDirectory",
      "x": "project_panel::Delete",
      "r": "project_panel::Rename",
      "escape": "workspace::ToggleLeftDock",
      "enter": ["workspace::SendKeystrokes", "enter escape"]
    }
  },
  {
    "context": "Dock && !editing",
    "bindings": {
      "space w h": "workspace::ActivatePaneLeft",
      "space w j": "workspace::ActivatePaneDown",
      "space w k": "workspace::ActivatePaneUp",
      "space w l": "workspace::ActivatePaneRight",
      "space w d": "workspace::ToggleLeftDock",
      "space t t": "workspace::ToggleLeftDock",
      "space p p": "projects::OpenRecent",
      "ctrl-p": "file_finder::Toggle"
    }
  },
  {
    "context": "AssistantPanel",
    "bindings": {
      "alt-j": null,
      "alt-k": null,
      "space w v": null,
      "space w s": null,
      "space E": null,
      "space e k": null,
      "space e j": null,
      "space e e": null,
      "space e f": null,
      "space e w": null,
      "space w h": null,
      "space w j": null,
      "space w k": null,
      "space w l": null,
      "space w H": null,
      "space w J": null,
      "space w L": null,
      "space w K": null,
      "space w d": null,
      "space b D": null,
      "space b d": null,
      "space b l": null,
      "space b b": null,
      "space tab": null,
      "space f f": null,
      "space t t": null,
      "space T c": null,
      "space p p": null,
      "space b o": null,
      "space b i": null,
      "space /": null,
      "space S": null,
      "space s": null,
      "space g b": null,
      "space g B": null
    }
  }
]
